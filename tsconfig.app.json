{
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "target": "ES6",
    "module": "ESNext",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": false,
  },
  "include": ["src"],
  "exclude": ["src/electron"]
}
